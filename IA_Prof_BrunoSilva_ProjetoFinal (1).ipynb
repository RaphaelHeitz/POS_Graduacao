    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "import seaborn as sns\n",
    "from sklearn.linear_model import LinearRegression\n",
    "from sklearn.ensemble import RandomForestClassifier, GradientBoostingClassifier\n",
    "from sklearn.metrics import accuracy_score, roc_auc_score, roc_curve, RocCurveDisplay, confusion_matrix, classification_report\n",
    "\n",
    "\n",
    "# lendo o arquivo CSV\n",
    "df = pd.read_csv('dados.csv', delimiter=';')\n",
    "df.rename({'CISP':'Area_Territorial', 'mes':'Mes', 'vano':'Ano','pol_militares_mortos_serv':'QTD_Militar','pol_civis_mortos_serv':'QTD_Civil'},axis=1,inplace=True)\n",
    "\n",
    "\n",
    "# selecionando as colunas relevantes\n",
    "X = df[['Mes', 'Ano']]\n",
    "y = df[['QTD_Militar', 'QTD_Civil']]\n",
    "\n",
    "# separando os dados em conjuntos de treinamento e teste\n",
    "X_train = X.iloc[:-5]\n",
    "y_train = y.iloc[:-5]\n",
    "X_test = X.iloc[-5:]\n",
    "y_test = y.iloc[-5:]\n",
    "\n",
    "# criando o modelo de regressão linear\n",
    "model = LinearRegression()\n",
    "\n",
    "# treinando o modelo com os dados de treinamento\n",
    "model.fit(X_train, y_train)\n",
    "\n",
    "# fazendo previsões com o modelo treinado\n",
    "y_pred = model.predict(X_test)\n",
    "\n",
    "# calculando o erro médio absoluto\n",
    "mae = abs(y_pred - y_test).mean()\n",
    "print(\"Erro médio absoluto:\", mae)\n",
    "\n",
    "# somando as categorias\n",
    "total_militares = df['QTD_Militar'].sum()\n",
    "total_civis = df['QTD_Civil'].sum()\n",
    "\n",
    "print(\"Total de mortes de policiais militares:\", total_militares)\n",
    "print(\"Total de mortes de policiais civis:\", total_civis)\n",
    "\n",
    "plt.figure(figsize=(12,6))\n",
    "fig = sns.heatmap(df.corr(), annot=True, cmap = 'coolwarm')\n",
    "\n"
